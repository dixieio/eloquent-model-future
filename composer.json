{
    "name": "dixieio/eloquent-model-future",
    "description": "Schedule changes for Eloquent models",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Ronni Egeriis Persson",
            "email": "ronni@egeriis.me"
        },
        {
            "name": "jstoone",
            "email": "jstoone@drk.sh"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Dixie\\EloquentModelFuture\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dixie\\EloquentModelFuture\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dixie\\EloquentModelFuture\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}

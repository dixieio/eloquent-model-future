{
    "name": "dixieio/laravel-model-future",
    "description": "A package that lets you plan future changes to a model.",
    "type": "library",
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "jstoone",
            "email": "jstoone@drk.sh"
        }
    ],
    "require": {
        "php": "^5.6|^7.0|^7.1",
        "laravel/framework": "^5.2|^5.3",
        "mockery/mockery": "^0.9.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Dixie\\EloquentModelFuture\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dixie\\EloquentModelFuture\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
